- if @interest.summary.present? or @interest.software?
  .row
    .medium-7.columns.medium-offset-5
      - if @interest.summary.present?
        %p.interest-summary= raw(@interest.summary)
      - if @interest.software?
        %p
          = render "resume/button"
          - if development?
            = link_to("Generate", resume_generator_path(format: :pdf), rel: "nofollow", data: {visible_to: 'admin'}, id: 'resume-generator-button', class: 'small button')
