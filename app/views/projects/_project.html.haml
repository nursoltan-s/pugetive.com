- cache project do
  %section.project.panel
    %h3= link_to(project.name, project)
    .subdued
      = project.titles.map{|t| t.name}.join(', ')
      = link_to(new_icon, new_role_path(project_id: project.id), data: {visible_to: 'admin'})
    = project.date_range
    %p= project.description
    = render "notes/list", subject: project
    = render "tools/list", project: project
